  background: #f1f1f1;
}
video{
  width: 100%;
  height: 427rpx;
}
.shi{
  width: 100%;
  height: 186rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.shi-con{
  width: 690rpx;
  height: 100%;
  overflow: hidden;
  margin: 0 auto;
}
.shi-con view:nth-child(1) {
  color: #292929;
  font-size: 15px;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
}
.shi-con view:nth-child(2) {
  color: #292929;
  font-size: 12px;
  margin-bottom: 10rpx;
}
.shi-con view:nth-child(3) {
  color: #8e8e8e;
  font-size: 12px;
}
.liu{
  width: 100%;
  height: 94rpx;
