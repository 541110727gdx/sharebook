/* pages/discount/discount.wxss */
page{
  background: #f5f5f5;
}
.give{
  width: 100%;
  background: #fff;
}
.give-tab{
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #e4e4e4;
}
.give-tab view{
  width: 33.333333%;
  height: 100%;
  line-height: 100rpx;
  color: #6f6f6f;
  font-size: 14px;
  float: left;
  text-align: center;
}
.text-act{
  display: inline-block;
  color: #63cc59;
  border-bottom: 1px solid #63cc59;
}
.dis{
  width: 100%; 
  margin-top: 30rpx;
}
.dis-con{
  width: 690rpx;
  margin: 0 auto;
  background: #f5f5f5;
}
.dis-con image{
  width: 100%;
  height: 233rpx;
  margin-bottom: 20rpx;
}
.youhui-main{
  width: 690rpx;
  height: 200rpx;
  margin: 0 auto 20rpx;
   background: #fff; 
  /* background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV1rZlgP/P2AYNZr5RSrFmaqoe26TWVB0P2KRkiGNgilr7lfMwnQ!/m/dFUAAAAAAAAAnull&bo=jAPwAAAAAAADB10!&rf=photolist&t=5) center center no-repeat;
  background-size: 100% 100%; */
}
.guoqi{
   background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV1rZlgP/N1Z3LTLUkDAZw.TvB3JFdO67576nl5VfFjeK5GIGisE!/m/dFkAAAAAAAAAnull&bo=pQB9AAAAAAADB*o!&rf=photolist&t=5) right center no-repeat;
  background-size: 165rpx 125rpx; 
  background-color: #fff;
}
.youhui-con{
  width: 35%;
  height: 100rpx;
  margin: 0 auto;
  float: left;
  line-height: 20px;
  margin-top: 50rpx;
}
.you-name{
  display: inline-block;
  width: 100%;
  color: #4f493a;
  font-size: 16px;
  text-align: center;
}
.you-time{
  display: inline-block;
  width: 100%;
  color: #4f493a;
  font-size: 10px;
  text-align: center;
}
.li{
  width: 45%;
  height: 100rpx;
  margin: 0 auto;
  float: left;
  line-height: 20px;
  margin-top: 50rpx;
}
.li view{
  width: 100%;
  color: #000;
  font-size: 14px;
  text-align: center;
}
.li view:nth-child(1) {
  color: #ff3d27;
  font-size: 22px;
  font-weight: bolder;
}
.li view:nth-child(2) {
  color: #ff3d27;
  font-size: 14px;
}













