.stepper {  
    width: 232rpx;  
    height: 60rpx;  
    /*给主容器设一个边框*/  
    border: 1px solid #999999;  
    border-radius: 3px;  
    margin:16rpx auto 0;  
    float: right;
}  
/*加号和减号*/  
.stepper text {  
    width: 60rpx;  
    line-height: 60rpx;  
    text-align: center;  
    float: left;  
    color: #999999;
}  
/*数值*/  
.stepper input {  
    width: 108rpx;  
    height: 60rpx;  
    float: left;  
    margin: 0 auto;  
    text-align: center;  
    font-size: 12px;  
    /*给中间的input设置左右边框即可*/  
    border-left: 1px solid #999999;  
    border-right: 1px solid #999999;  
    color: #000;
}  
/*普通样式*/  
.stepper .normal{  
    color: black;  
}  
/*禁用样式*/  
.stepper .disabled{  
    color: #999999;  
}  
